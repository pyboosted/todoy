{
  "name": "Todoy",
  "version": "0.0.1",
  "description": "Just another one todo app.",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules dist/client && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "watch-server": "tsc -p tsconfig-server.json -w",
    "build": "rimraf dist/client && webpack --progress --profile --bail",
    "build-server": "rimraf dist/server && tsc -p tsconfig-server.json",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "start": "npm run server"
  },
  "dependencies": {
    "@angular/common": "4.0.0-beta.4",
    "@angular/compiler": "4.0.0-beta.4",
    "@angular/core": "4.0.0-beta.4",
    "@angular/forms": "4.0.0-beta.4",
    "@angular/http": "4.0.0-beta.4",
    "@angular/platform-browser": "4.0.0-beta.4",
    "@angular/platform-browser-dynamic": "4.0.0-beta.4",
    "@angular/router": "4.0.0-beta.4",
    "core-js": "^2.4.1",
    "mongodb": "^3.6.1",
    "monk": "^3.1.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.1",
    "typeix": "^1.0.0-beta.1",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/node": "^7.0.0",
    "@types/monk": "^1.0.5",
    "@types/mongodb": "^2.1.37",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.3.2",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "copy-webpack-plugin": "^4.0.0",
    "css-less-loader": "^0.1.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "json-loader": "^0.5.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^3.4.2",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.2.2",
    "url-loader": "^0.5.7",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.1",
    "shelljs": "^0.7.0",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "2.1",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  }
